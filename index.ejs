<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Web Component Test</title>
    <style type="text/css">
        body{
            background-color: #888;
            background-image: linear-gradient(45deg, #638f46 25%, #3c692b 25%, #3c692b 50%, #638f46 50%, #638f46 75%, #3c692b 75%, #3c692b 100%);
            background-size: 56.57px 56.57px;
            color: #fff;
            font-family: Arial, Helvetica, sans-serif;
        }
        a, a:visited, a:active, a:hover{
            color: #fff;
        }
    </style>
</head>
<body>
    <div class="container">
        <list-lists dataBind="lists"></list-lists>
        <gz-if dataBind="state.activeList" bind-child="byref">
            <div slot="then">
                <list-items dataBind=""></list-items>
            </div>
            <div slot="else">
                <h1>Choose a list</h1>
            </div>
        </gz-if>
        <list-item-detail dataBind=""></list-item-detail>
        <gz-badge></gz-badge>
	</div>
</body>
</html>